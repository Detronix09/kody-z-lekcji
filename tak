#include <iostream>
using namespace std;

float f(float x) {
    return x * x * x - 3 * x * x + 2 * x - 6;
}

int main(){
    float a = -10, b = 10, eps = 0.0001, s, wb;
    s = (a + b) / 2;
    wb = a - b;
    if (wb < 0) wb *= -1;
    while (wb > eps) {
        if (f(a) * f(s) < 0) b = s;

    }
    cout << f(4);
}
